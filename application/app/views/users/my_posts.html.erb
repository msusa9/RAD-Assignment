<div id = 'sidebar'>
    <%= render 'settings_sidebar'%>
</div>

<div id="Posts">
    <h2>My Posts</h2>
    <table id="profTable">
      <% @posts.each do |post| %>
        <tr>
          <td><%= link_to post.title, post, title: post.title %></td>
          <td><%= post.created_at.to_date %></td>
          <td><%= link_to 'Delete Post', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </table>
  </div>